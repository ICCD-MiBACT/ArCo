<rdf:Description rdf:about="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/009.021.002.002">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="it">ostrakon</rdfs:label>
	<skos:broader rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/009.021.002"/>
	<skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300185396"/>
	<skos:definition xml:lang="it">Frammento di vasellame o pietra utilizzata come supporto di una scrittura a inchiostro.</skos:definition>
