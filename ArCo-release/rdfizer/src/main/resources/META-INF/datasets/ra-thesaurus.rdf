	<skos:narrower rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/009.003.003.027.001"/>
	<skos:narrower rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/009.003.003.027.002"/>
	<skos:prefLabel xml:lang="it">guanto</skos:prefLabel>
