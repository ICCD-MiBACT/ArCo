	<skos:prefLabel xml:lang="it">corno</skos:prefLabel>
	<foaf:depiction rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/immagini//th-ra_009.019.000.004.jpg" />
</rdf:Description>

