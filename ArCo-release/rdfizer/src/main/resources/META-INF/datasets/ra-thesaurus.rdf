	<skos:prefLabel xml:lang="it">catena (materiali da costruzione)</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/003.005.000.003">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="it">cavicchio</rdfs:label>
	<skos:altLabel xml:lang="it">caviglia</skos:altLabel>
	<skos:broader rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/003.005"/>
	<skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300068604"/>
	<skos:definition xml:lang="it">Sorta di grossa vite di bronzo utilizzata in carpenteria per fissare le commessure assicurando l'assemblaggio dei diversi componenti della capriata lignea.</skos:definition>
	<skos:editorialNote xml:lang="it">Fonte: Dizionario oggetto (OGTD-OGTT): Manufatti per l’edilizia e le infrastrutture in metallo e leghe metalliche (non pubblicato)</skos:editorialNote>
	<skos:inScheme rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/"/>
	<skos:prefLabel xml:lang="it">cavicchio</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/003.005.000.004">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="it">cerchio</rdfs:label>
