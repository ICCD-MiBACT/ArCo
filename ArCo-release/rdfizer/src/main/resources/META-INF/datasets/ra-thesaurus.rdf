	<skos:prefLabel xml:lang="it">ghianda (armi da lancio)</skos:prefLabel>
	<foaf:depiction rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/immagini//th-ra_009.003.005.006.jpg" />
</rdf:Description>

