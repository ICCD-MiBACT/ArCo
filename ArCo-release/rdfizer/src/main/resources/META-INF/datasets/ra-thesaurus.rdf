	<rdfs:label xml:lang="it">catena</rdfs:label>
	<skos:broader rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/003.005"/>
	<skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300079218"/>
