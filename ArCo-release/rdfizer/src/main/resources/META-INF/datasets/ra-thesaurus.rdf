<rdf:Description rdf:about="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/001.001.000.006">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="it">brattea</rdfs:label>
