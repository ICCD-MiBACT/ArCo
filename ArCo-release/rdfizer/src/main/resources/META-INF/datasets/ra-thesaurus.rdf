<rdf:Description rdf:about="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/001.005.000.010.001">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="it">esomide</rdfs:label>
	<skos:broader rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/001.005.000.010"/>
