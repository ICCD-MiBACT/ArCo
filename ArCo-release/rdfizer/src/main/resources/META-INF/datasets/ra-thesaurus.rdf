	<skos:inScheme rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/"/>
	<skos:prefLabel xml:lang="it">maglia ghiazzerina</skos:prefLabel>
	<foaf:depiction rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/immagini//th-ra_009.003.003.029.004.jpg" />
</rdf:Description>

<rdf:Description rdf:about="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/009.003.003.030">
