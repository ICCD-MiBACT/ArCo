	<skos:narrower rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/009.003.003.034.002"/>
	<skos:narrower rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/009.003.003.034.003"/>
	<skos:prefLabel xml:lang="it">piastra</skos:prefLabel>
	<foaf:depiction rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/immagini//th-ra_009.003.003.034.jpg" />
</rdf:Description>

