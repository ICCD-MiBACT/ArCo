	<skos:prefLabel xml:lang="it">elmo a maschera</skos:prefLabel>
	<foaf:depiction rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/immagini//th-ra_009.003.003.019.012.jpg" />
