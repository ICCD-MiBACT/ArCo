	<skos:narrower rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/004.001.000.001.001"/>
	<skos:prefLabel xml:lang="it">quinquireme</skos:prefLabel>
	<foaf:depiction rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/immagini//th-ra_004.001.000.001.jpg" />
</rdf:Description>

