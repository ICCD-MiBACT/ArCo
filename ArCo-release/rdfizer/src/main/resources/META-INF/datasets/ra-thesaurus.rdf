	<skos:prefLabel xml:lang="it">pestello</skos:prefLabel>
</rdf:Description>

<rdf:Description rdf:about="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/009.023.000.015.001">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:label xml:lang="it">da mortaio</rdfs:label>
	<skos:broader rdf:resource="http://dati.beniculturali.it/vocabularies/reperti_archeologici/def/009.023.000.015"/>
	<skos:closeMatch rdf:resource="http://vocab.getty.edu/page/aat/300024725"/>
