				<!-- transferor of acquisition as an individual and previuous owner -->
				<xsl:for-each select="./ACQN">
					<xsl:if test=". and (not(starts-with(lower-case(normalize-space(.)), 'nr')) and not(starts-with(lower-case(normalize-space(.)), 'n.r')))">
