										<xsl:call-template name="CamelCase">
											<xsl:with-param name="text" select="normalize-space(./PRV/PRVS)" />
										</xsl:call-template>
									</rdfs:label>
									<l0:name>
										<xsl:call-template name="CamelCase">
											<xsl:with-param name="text" select="normalize-space(./PRV/PRVS)" />
										</xsl:call-template>
									</l0:name>
