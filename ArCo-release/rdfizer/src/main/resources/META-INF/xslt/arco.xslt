                				<xsl:value-of select="concat($NS, 'SurveyType/', arco-fn:urify(normalize-space(./AINT)))" />
                				</xsl:attribute>
							<rdf:type rdf:resource="https://w3id.org/arco/ontology/context-description/SurveyType" />
							<rdfs:label>
								<xsl:value-of select="normalize-space(./AINT)" />
							</rdfs:label>
							<l0:name>
								<xsl:value-of select="normalize-space(./AINT)" />
							</l0:name>
