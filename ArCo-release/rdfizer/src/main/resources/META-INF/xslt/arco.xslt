			<!-- affixed element (*/LSI) -->
			<xsl:if test="record/metadata/schede/*/*/LSI/*">
				<xsl:for-each select="record/metadata/schede/*/*/LSI">
					<xsl:variable name="parentPosition">
						<xsl:value-of select="position()" />
					</xsl:variable>
