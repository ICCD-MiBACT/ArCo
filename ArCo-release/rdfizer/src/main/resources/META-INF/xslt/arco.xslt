							<xsl:attribute name="rdf:about">
								<xsl:value-of select="concat($NS, 'Agent/', arco-fn:arcofy(./STMI))" />
	                            </xsl:attribute>
							<rdf:type rdf:resource="https://w3id.org/italia/onto/l0/Agent" />
							<rdfs:label>
								<xsl:value-of select="normalize-space(./STMI)" />
							</rdfs:label>
							<l0:name>
								<xsl:value-of select="normalize-space(./STMI)" />
							</l0:name>
						</rdf:Description>
					</xsl:if>
