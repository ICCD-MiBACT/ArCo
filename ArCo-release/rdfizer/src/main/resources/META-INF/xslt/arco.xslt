						</xsl:for-each>
					</rdf:Description>
					<!-- affixed element type as an individual -->
					<xsl:choose>
						<xsl:when test="./ISEE and (not(starts-with(lower-case(normalize-space(./ISEE)), 'nr')) and not(starts-with(lower-case(normalize-space(./ISEE)), 'n.r')))">
