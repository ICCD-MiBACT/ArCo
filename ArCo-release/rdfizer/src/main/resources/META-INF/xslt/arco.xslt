							<arco-core:hasType>
								<xsl:attribute name="rdf:resource">
                					<xsl:value-of select="concat($NS, 'SurveyType/', arco-fn:urify(normalize-space(./AINT)))" />
                				</xsl:attribute>
							</arco-core:hasType>
