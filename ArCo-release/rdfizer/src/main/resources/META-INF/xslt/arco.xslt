			<!-- Participant role - Compilation -->
			<xsl:for-each
				select="record/metadata/schede/*/CM/CMP/CMPN">
				<xsl:if
					test=". and (not(starts-with(lower-case(normalize-space(.)), 'nr')) and not(starts-with(lower-case(normalize-space(.)), 'n.r')))">
					<rdf:Description>
