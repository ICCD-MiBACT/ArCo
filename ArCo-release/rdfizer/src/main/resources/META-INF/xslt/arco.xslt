								</xsl:otherwise>
							</xsl:choose>
						</rdfs:label>
						<l0:name>
							<xsl:choose>
 	                        	<xsl:when test="./PVLT">
									<xsl:value-of select="normalize-space(./PVLT)" />
								</xsl:when>
								<xsl:otherwise>
									<xsl:value-of select="normalize-space(.)" />
								</xsl:otherwise>
							</xsl:choose>
