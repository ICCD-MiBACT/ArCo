						<rdfs:label xml:lang="en">
							<xsl:value-of select="concat('Heritage protection agency for cultural property ', $itemURI, ': ', arco-fn:get-nome-ente-from-codice(record/metadata/schede/harvesting/enteCompetente))" />
						</rdfs:label>
						<arco-core:hasRole>
