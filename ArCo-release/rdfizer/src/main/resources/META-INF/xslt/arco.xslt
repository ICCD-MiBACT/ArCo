					<!-- copy as an individual -->
					<rdf:Description>
						<xsl:attribute name="rdf:about">
										<xsl:value-of
							select="concat($NS, 'Copy/', $itemURI, '-copy-', position())" />
									</xsl:attribute>
						<rdf:type>
							<xsl:attribute name="rdf:resource">
		                          		<xsl:value-of
