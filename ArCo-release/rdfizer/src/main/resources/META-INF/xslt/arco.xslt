										<xsl:when test="./AUFB">
											<xsl:choose>
												<xsl:when test="$sheetType='F' and (./AUFS)">
													<xsl:call-template name="CamelCase">
														<xsl:with-param name="text" select="concat(normalize-space(./AUFB), ' (', normalize-space(./AUFS), ')')" />
													</xsl:call-template>
												</xsl:when>
