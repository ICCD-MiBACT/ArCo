							<arco-cd:hasAuthor>
								<xsl:attribute name="rdf:resource">
			            			<xsl:choose>
			            				<xsl:when test="../CRF/CRFN">
			            					<xsl:value-of select="concat($NS, 'Agent/', arco-fn:arcofy(../CRF/CRFN))" />
			            				</xsl:when>
			            				<xsl:otherwise>
			            					<xsl:value-of select="concat($NS, 'Agent/', arco-fn:arcofy(../CRF/CRFB))" />
			            				</xsl:otherwise>
			            			</xsl:choose>
			            		</xsl:attribute>
							</arco-cd:hasAuthor>
							<dc:creator>
								<xsl:attribute name="rdf:resource">
			            			<xsl:choose>
			            				<xsl:when test="../CRF/CRFN">
			            					<xsl:value-of select="concat($NS, 'Agent/', arco-fn:arcofy(../CRF/CRFN))" />
			            				</xsl:when>
			            				<xsl:otherwise>
			            					<xsl:value-of select="concat($NS, 'Agent/', arco-fn:arcofy(../CRF/CRFB))" />
