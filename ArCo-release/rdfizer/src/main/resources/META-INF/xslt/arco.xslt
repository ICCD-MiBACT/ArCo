								<xsl:value-of select="normalize-space(./PVLR)" />
							</arco-arco:endTime>
							<rdfs:label xml:lang="it">
								<xsl:value-of select="concat(./PVLR, ' - ', ./PVLR)" />
							</rdfs:label>
