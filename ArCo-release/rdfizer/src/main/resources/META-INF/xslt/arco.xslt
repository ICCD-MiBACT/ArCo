			<!-- AU/AFB (F version 2.00, 3.00) -->
			<xsl:for-each select="record/metadata/schede/*/AU/AFB">
				<xsl:if test="(not(starts-with(lower-case(normalize-space(./AFBD)), 'nr')) and not(starts-with(lower-case(normalize-space(./AFBD)), 'n.r')))">
