         <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
         <a-cat:iccdNormTag>SM/SMM/SMMA</a-cat:iccdNormTag>
     </owl:ObjectProperty>



    <!-- https://w3id.org/arco/ontology/arco-lite/hasAttributedAuthor -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco-lite/hasAttributedAuthor">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/hasRelatedAgent"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/isAttributedAuthorOf"/>
