        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco-lite"/>
        <rdfs:label xml:lang="it">ha durezza secondo la scala di Mohs</rdfs:label>
        <rdfs:label xml:lang="en">has Mohs scale hardness</rdfs:label>
