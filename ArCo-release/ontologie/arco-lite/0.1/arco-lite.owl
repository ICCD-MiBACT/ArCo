    <!-- https://w3id.org/arco/ontology/arco-lite/hasExportOffice -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco-lite/hasExportOffice">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/hasRelatedAgent"/>
