     <!-- https://w3id.org/arco/ontology/arco-lite/hasAnisotropy -->

     <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco-lite/hasAnisotropy">
         <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/hasOpticCharacteristic"/>
