    <!-- https://w3id.org/arco/ontology/arco-lite/hasSoundEngineer -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco-lite/hasSoundEngineer">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/hasRelatedAgent"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/isSoundEngineerOf"/>
