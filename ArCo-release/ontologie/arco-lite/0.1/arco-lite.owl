    <!-- https://w3id.org/arco/ontology/arco-lite/hasMint -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco-lite/hasMint">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/hasRelatedAgent"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/isMintIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/context-description/CoinIssuance"/>
