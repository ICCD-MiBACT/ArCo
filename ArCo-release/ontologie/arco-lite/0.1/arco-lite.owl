    <!-- https://w3id.org/arco/ontology/arco-lite/hasRelatedAgent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco-lite/hasRelatedAgent">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/isRelatedAgentTo"/>
