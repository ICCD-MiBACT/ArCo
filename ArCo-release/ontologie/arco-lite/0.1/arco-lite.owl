    <!-- https://w3id.org/arco/ontology/arco-lite/hasPreviousLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco-lite/hasPreviousLocation">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/hasRelatedWork"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/isPreviousLocationOf"/>
