    <!-- https://w3id.org/arco/ontology/arco-lite/hasPhotographer -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco-lite/hasPhotographer">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/hasRelatedAgent"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/arco-lite/isPhotographerOf"/>
