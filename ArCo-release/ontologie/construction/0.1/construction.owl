        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/immovable-property"/>
        <rdfs:label xml:lang="it">ha spazio della costruzione</rdfs:label>
        <rdfs:label xml:lang="en">has construction space</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD norm MA, CA, SI, SAS: DA/SPA</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction/hasCovering -->

