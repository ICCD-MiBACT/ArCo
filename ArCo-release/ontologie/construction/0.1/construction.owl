        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction/Covering"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità al rivestimento  .</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects an entity to a covering .</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/immovable-property"/>
        <rdfs:label xml:lang="it">ha rivestimento</rdfs:label>
        <rdfs:label xml:lang="en">has covering</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


