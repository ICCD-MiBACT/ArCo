    <!-- https://w3id.org/arco/ontology/construction/hasArchitecturalOrder -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction/hasArchitecturalOrder">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/denotative-description/hasTechnicalCharacteristic"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction/isArchitecturalOrderOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/construction/VerticalElement"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction/ArchitecturalOrder"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità all&apos;ordine architettonico.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects an entity to the architectural order.</rdfs:comment>
        <rdfs:label xml:lang="it">ha ordine architettonico</rdfs:label>
        <rdfs:label xml:lang="en">has architectural order</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction/hasConstructionElement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction/hasConstructionElement">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/core/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction/isConstructionElementOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/construction/Construction"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction/ConstructionElement"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una costruzione all&apos;elemento della costruzione.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects a construction to the construction element.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/immovable-property"/>
        <rdfs:label xml:lang="it">ha elemento della costruzione</rdfs:label>
        <rdfs:label xml:lang="en">has construction element</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction/hasConstructionSpace -->

