        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa Trasversale 4.00: CT/CTM/CTMC</a-cat:iccdNormTag>
    </owl:Class>
    


