        <rdfs:comment xml:lang="it">Questa classe rappresenta un bene materiale, cioè fisicamente tangibile, sia mobile che immobile.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents a tangible cultural property, either immovable or movable.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco"/>
        <rdfs:label xml:lang="it">Bene materiale</rdfs:label>
        <rdfs:label xml:lang="en">Tangible Cultural Property</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:Class>
    


    <!-- https://w3id.org/arco/ontology/arco/ThematicCategory -->

    <owl:Class rdf:about="https://w3id.org/arco/ontology/arco/ThematicCategory">
