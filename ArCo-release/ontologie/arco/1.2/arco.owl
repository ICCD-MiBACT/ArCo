        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <a-cat:iccdNormTag xml:lang="it">&quot;Riferimento alla parte&quot;, presente in molti campi strutturati delle normative ICCD - ICCD Normativa AT 3.01: EL/ELE</a-cat:iccdNormTag>
    </owl:Class>
    


    <!-- https://w3id.org/arco/ontology/arco/CulturalPropertyResidual -->

