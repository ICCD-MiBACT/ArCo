        <rdfs:label xml:lang="en">Cultural property inventory category</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa Trasversale 4.00: OG/OGT/OGTS</a-cat:iccdNormTag>
    </owl:Class>
    


    <!-- https://w3id.org/arco/ontology/arco/CulturalPropertyPart -->

    <owl:Class rdf:about="https://w3id.org/arco/ontology/arco/CulturalPropertyPart">
