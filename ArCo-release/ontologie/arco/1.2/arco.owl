    </owl:Class>
    


    <!-- https://w3id.org/arco/ontology/arco/RFId -->

    <owl:Class rdf:about="https://w3id.org/arco/ontology/arco/RFId">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/arco/ontology/arco/isRFIdOf"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/arco/ontology/core/note"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
