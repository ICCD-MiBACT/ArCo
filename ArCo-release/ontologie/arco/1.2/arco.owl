        <rdfs:comment xml:lang="it">Questa classe rappresenta l&apos;identificatore a radiofrequenza RFId.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents the radio-frequency identification (RFID).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco"/>
        <rdfs:label xml:lang="it">Identificativo RFId</rdfs:label>
        <rdfs:label xml:lang="en">RFId Identifier</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa Trasversale 4.00: RF/RFI</a-cat:iccdNormTag>
    </owl:Class>
    


    <!-- https://w3id.org/arco/ontology/arco/ReferenceCoinLegend -->

    <owl:Class rdf:about="https://w3id.org/arco/ontology/arco/ReferenceCoinLegend">
