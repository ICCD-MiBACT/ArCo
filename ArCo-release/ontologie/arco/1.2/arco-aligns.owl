    <!-- http://www.europeana.eu/schemas/edm/PhysicalThing -->

    <owl:Class rdf:about="http://www.europeana.eu/schemas/edm/PhysicalThing"/>
    


    <!-- http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject -->

    <owl:Class rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject">
        <owl:equivalentClass rdf:resource="https://w3id.org/arco/ontology/arco/TangibleCulturalProperty"/>
    </owl:Class>
    


    <!-- https://w3id.org/arco/ontology/arco/MusicHeritage -->

    <owl:Class rdf:about="https://w3id.org/arco/ontology/arco/MusicHeritage">
        <rdfs:subClassOf rdf:resource="http://id.loc.gov/ontologies/bibframe/MusicInstrument"/>
    </owl:Class>
    


    <!-- https://w3id.org/arco/ontology/arco/TangibleCulturalProperty -->

    <owl:Class rdf:about="https://w3id.org/arco/ontology/arco/TangibleCulturalProperty">
