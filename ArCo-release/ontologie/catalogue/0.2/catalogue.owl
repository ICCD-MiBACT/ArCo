        <rdfs:label xml:lang="en">has digital transcription operator</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>CM/RVM/RVMN</core:iccdNormTag>
    </owl:ObjectProperty>
    


