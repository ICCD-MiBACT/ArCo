            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">Questa classe rappresenta la classificazione di strumenti musicali</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents the classification of musical instrument</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="it">Classificazione di strumenti musicali</rdfs:label>
        <rdfs:label xml:lang="en">Musical instrument class</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa SM 4.00: CL/CLA/CLAT</a-cat:iccdNormTag>
    </owl:Class>
    


    <!-- https://w3id.org/arco/ontology/musical-instrument/MusicalInstrumentTaxon -->

    <owl:Class rdf:about="https://w3id.org/arco/ontology/musical-instrument/MusicalInstrumentTaxon">
