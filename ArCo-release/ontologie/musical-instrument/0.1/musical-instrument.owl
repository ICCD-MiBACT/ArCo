    <!-- https://w3id.org/arco/ontology/denotative-description/hasMeasurementCollection -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/denotative-description/hasMeasurementCollection"/>
    


    <!-- https://w3id.org/arco/ontology/musical-instrument/hasString -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/musical-instrument/hasString">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/musical-instrument/isStringOf"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega uno strumento musicale con la sua corda</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a musical instrument with its string</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="it">ha corda</rdfs:label>
        <rdfs:label xml:lang="en">has string</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa SM 3.01: AA/AAL</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/musical-instrument/involvesAccompanyingInstrument -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/musical-instrument/involvesAccompanyingInstrument">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/musical-instrument/isAccompanyingInstrumentInvolvedIn"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/musical-instrument/MusicalInstrument"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità al su strumento musicale di accompagnamento</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates an entity with its accompanying instrument</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="it">comprende strumento di accompagnamento</rdfs:label>
        <rdfs:label xml:lang="en">involves accompanying instrument</rdfs:label>
        <owl:versionInfo xml:lang="it">instable</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/musical-instrument/involvesSoloInstrument -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/musical-instrument/involvesSoloInstrument">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/musical-instrument/isSoloInstrumentInvolvedIn"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/musical-instrument/MusicalInstrument"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità a uno strumento musicale solista</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates an entity with a solo musical instrument</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="it">coinvolge strumento solista</rdfs:label>
        <rdfs:label xml:lang="en">involves solo instrument</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/musical-instrument/isAccompanyingInstrumentInvolvedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/musical-instrument/isAccompanyingInstrumentInvolvedIn">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/musical-instrument/MusicalInstrument"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="it">Questa proprietà collega uno strumento musicale di accompagnamento con un&apos;entità</rdfs:label>
        <rdfs:label xml:lang="it">This property relates an accompanying musical instrument with an entity</rdfs:label>
        <rdfs:label xml:lang="en">is accompanying instrument involved in</rdfs:label>
        <rdfs:label xml:lang="it">è strumento musicale di accompagnamento coinvolto in</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/musical-instrument/isProducedByMusicalInstrument -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/musical-instrument/isProducedByMusicalInstrument">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/musical-instrument/producesMusicalNote"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/musical-instrument/MusicalInstrument"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/musical-instrument/MusicalNote"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una nota musicale allo strumento musicale che la produce</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a musical note to the instrument that produces it</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="en">is produced by musical instrument</rdfs:label>
        <rdfs:label xml:lang="it">è prodotta da strumento musicale</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/musical-instrument/isSoloInstrumentInvolvedIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/musical-instrument/isSoloInstrumentInvolvedIn">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/musical-instrument/MusicalInstrument"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega uno strumento musicale solista a un&apos;altra entità</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a solo musical instrument to an entity</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="en">is solo instrument involved in</rdfs:label>
        <rdfs:label xml:lang="it">è strumento musicale solista coinvolto in</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/musical-instrument/isStringOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/musical-instrument/isStringOf">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/musical-instrument/String"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/musical-instrument/MusicalInstrument"/>
        <rdfs:comment xml:lang="it">Questa è la proprietà inversa di &quot;ha corda&quot;</rdfs:comment>
        <rdfs:comment xml:lang="en">This is the inverse property of &quot;has string&quot;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="en">is string of</rdfs:label>
        <rdfs:label xml:lang="it">è corda di</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa SM 3.01: AA/AAL</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/musical-instrument/producesMusicalNote -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/musical-instrument/producesMusicalNote">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/musical-instrument/MusicalInstrument"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/musical-instrument/MusicalNote"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega uno strumento musicale a una nota musicale che è in grado di produrre</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a musical instrument with a musical note</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="it">produce nota musicale</rdfs:label>
        <rdfs:label xml:lang="en">produces musical note</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


