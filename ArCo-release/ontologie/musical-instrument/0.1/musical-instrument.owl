        <rdfs:comment xml:lang="en">This property relates the Hornbostel-Sachs class with its definition</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="en">Hornbostel-Sachs definition</rdfs:label>
        <rdfs:label xml:lang="it">definizione Hornbostel-Sachs</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa SM 4.00: CL/CHS/CHSD</a-cat:iccdNormTag>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/arco/ontology/musical-instrument/HSEdition -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/arco/ontology/musical-instrument/HSEdition">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/musical-instrument/HornbostelSachsClass"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la classificazione Horbostel-Sachs al riferimento bibliografico dell&apos;edizione scelta</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates the Hornbostel-Sachs class with the used edition</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="en">Hornbostel-Sachs edition</rdfs:label>
        <rdfs:label xml:lang="it">edizione Hornbostel-Sachs</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa SM 4.00: CL/CHS/CHSE</a-cat:iccdNormTag>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/arco/ontology/musical-instrument/HSNumber -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/arco/ontology/musical-instrument/HSNumber">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/l0/identifier"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/musical-instrument/HornbostelSachsClass"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la classe Hornbostel-Sachs al suo rispettivo codice numerico</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates the Hornbostel-Sachs class with its respective identifier number</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/musical-instrument"/>
        <rdfs:label xml:lang="en">Hornbostel-Sachs number</rdfs:label>
        <rdfs:label xml:lang="it">codice numerico Hornbostel-Sachs</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa SM 4.00: CL/CHS/CHSN</a-cat:iccdNormTag>
    </owl:DatatypeProperty>
    


