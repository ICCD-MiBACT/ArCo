<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/arco/location/"
     xml:base="https://w3id.org/arco/location/"
     xmlns:arco-core="https://w3id.org/arco/core/"
     xmlns:adms="https://w3id.org/italia/onto/ADMS/"
     xmlns:l0="https://w3id.org/italia/onto/l0/"
     xmlns:arco-location="https://w3id.org/arco/location/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:ns="http://www.w3.org/2006/vcard/ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:cis="http://dati.beniculturali.it/cis/"
     xmlns:clv="https://w3id.org/italia/onto/CLV/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:dcat="http://www.w3.org/ns/dcat#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://w3id.org/arco/location/">
        <owl:imports rdf:resource="http://dati.beniculturali.it/cis/"/>
        <owl:imports rdf:resource="https://w3id.org/arco/core/"/>
        <dc:creator>Chiara Veninata (MiBACT, ICCD)</dc:creator>
        <dc:creator>Aldo Gangemi (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Valentina Carriero (ISTC-CNR, STLab)</dc:creator>
        <owl:versionInfo xml:lang="it">Versione 0.3 beta</owl:versionInfo>
        <dc:creator>Andrea Nuzzolese (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Marialetizia Mancinelli (MiBACT, ICCD)</dc:creator>
        <dc:creator>Valentina Presutti (ISTC-CNR, STLab)</dc:creator>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/arco/core/iccdNormTag -->

    <owl:AnnotationProperty rdf:about="https://w3id.org/arco/core/iccdNormTag"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dati.beniculturali.it/cis/isInSite -->

    <owl:ObjectProperty rdf:about="http://dati.beniculturali.it/cis/isInSite"/>
    


    <!-- https://w3id.org/arco/location/atLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/atLocation">
        <rdfs:domain rdf:resource="https://w3id.org/arco/location/TimeIndexedQualifiedLocation"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/Feature"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una localizzazione qualificata nel tempo al luogo cui si riferisce.</rdfs:comment>
        <rdfs:label xml:lang="en">at location</rdfs:label>
        <rdfs:label xml:lang="it">presso luogo</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">se LC senza sottoelemento LDC; se LA senza sottoelemento PRC</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/atSite -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/atSite">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/location/atLocation"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/location/TimeIndexedQualifiedLocation"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/Site"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una localizzazione qualificata nel tempo al contenitore fisico cui si riferisce.</rdfs:comment>
        <rdfs:label xml:lang="en">at site</rdfs:label>
        <rdfs:label xml:lang="it">presso contenitore fisico</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag>LC/LDC; LA/PRC</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasAltitude -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasAltitude">
        <rdfs:domain rdf:resource="https://w3id.org/arco/location/Coordinates"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/location/Altitude"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega le coordinate relative alla georeferenziazione di un oggetto (Geometry) alla sua altitudine.</rdfs:comment>
        <rdfs:label xml:lang="it">ha altitudine</rdfs:label>
        <rdfs:label xml:lang="en">has altitude</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">quota minima: GP/GPC/GPCI - GA/GAQ/GAQI - GL/GLQ/GLQI - MT/MTA/MTAM; quota massima: GP/GPC/GPCS - GA/GAQS - GL/GLQ/GLQS - MT/MTA/MTAX; quota relativa: MT/MTA/MTAR; coordinata z: 4.00 GE/GEC/GECZ - 3.00GP/GPC/GPCL</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasBaseMap -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasBaseMap">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Geometry"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/location/BaseMap"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una georeferenziazione di un oggetto alla base cartografica eventualmente utilizzata.</rdfs:comment>
        <rdfs:label xml:lang="it">ha base cartografica</rdfs:label>
        <rdfs:label xml:lang="en">has base map</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: GE/GPB; versioni precedenti: GP/GPB - GA/GAB - GL/GLB</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasContinent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasContinent">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/location/Continent"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la localizzazione di un&apos;entità al continente in cui si trova.</rdfs:comment>
        <rdfs:label xml:lang="it">ha continente</rdfs:label>
        <rdfs:label xml:lang="en">has continent</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>LA/PRT/PRTK</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasCoordinates -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasCoordinates">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Geometry"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/location/Coordinates"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la georeferenziazione di un oggetto (Geometry) alle coordinate (x, y, z).</rdfs:comment>
        <rdfs:label xml:lang="it">ha coordinate</rdfs:label>
        <rdfs:label xml:lang="en">has coordinates</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag>GP/GPD; GA/GAPD; GL/GLS, ICCD Normativa Trasversale: GE/GEC</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasCulturalPropertyAddress -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasCulturalPropertyAddress">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="http://dati.gov.it/onto/clvapit#Address"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione che collega un bene culturale all&apos;indirizzo della sua localizzazione fisica attuale, al momento della catalogazione. La relazione fornisce una rappresentazione semplificata, con ridotta espressività, della localizzazione attuale del bene culturale, e provoca la perdita delle altre informazioni che riguardano la collocazione del bene (eventuale contenitore fisico, continente, collocazione specifica nel contenitore fisico, etc.)</rdfs:comment>
        <rdfs:label xml:lang="en">cultural property address</rdfs:label>
        <rdfs:label xml:lang="it">indirizzo del bene culturale</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>LC/PVC</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasGeometryMethod -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasGeometryMethod">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Geometry"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/location/GeometryMethod"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una georeferenziazione al metodo utilizzato per il posizionamento.</rdfs:comment>
        <rdfs:label xml:lang="it">ha metodo di georeferenziazione</rdfs:label>
        <rdfs:label xml:lang="en">has geometry method</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: GE/GPM; versioni precedenti: GP/GPM - GA/GAM - GL/GLM</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasGeometryTechnique -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasGeometryTechnique">
        <rdfs:label xml:lang="it">ha tecnica di georeferenziazione</rdfs:label>
        <rdfs:label xml:lang="en">has geometry technique</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: GE/GPT; versioni precedenti: GP/GPT - GA/GAT - GL/GLT</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasLocationType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasLocationType">
        <rdfs:domain rdf:resource="https://w3id.org/arco/location/TimeIndexedQualifiedLocation"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/location/LocationType"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una localizzazione qualificata nel tempo al tipo di localizzazione.</rdfs:comment>
        <rdfs:label xml:lang="it">ha tipo di localizzazione</rdfs:label>
        <rdfs:label xml:lang="en">has location type</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">LA/TLC; LA/TCL; LC (CurrentPhysicalLocation); per normativa F: anche LR (ShotLocation)</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasReferredLocationType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasReferredLocationType">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Geometry"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/location/LocationType"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una georeferenziazione al tipo di localizzazione (fisica attuale, precedente, di rinvenimento, etc.) cui si riferisce.</rdfs:comment>
        <rdfs:label xml:lang="it">ha tipo di localizzazione cui si riferisce</rdfs:label>
        <rdfs:label xml:lang="en">has referred location type</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: GE/GEL; Normative di versioni precedenti: GP/GPL; GA/GAL; GL/GLL</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasSiteDefinition -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasSiteDefinition">
        <rdfs:domain rdf:resource="https://w3id.org/arco/location/SiteType"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/location/SiteDefinition"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la tipologia di un contenitore fisico a una delle sue componenti, cioè alla definizione della sua tipologia.</rdfs:comment>
        <rdfs:label xml:lang="it">ha definizione del contenitore fisico</rdfs:label>
        <rdfs:label xml:lang="en">has site definition</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>LC/LDC/LDCT</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasSiteSpecification -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasSiteSpecification">
        <rdfs:domain rdf:resource="https://w3id.org/arco/location/SiteType"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/location/SiteSpecification"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la tipologia di un contenitore fisico a una delle sue componenti, cioè alle specifiche sulla sua tipologia.</rdfs:comment>
        <rdfs:label xml:lang="it">ha qualificazione del contenitore fisico</rdfs:label>
        <rdfs:label xml:lang="en">has site specification</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>LC/LDC/LDCQ</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasSiteType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasSiteType">
        <rdfs:domain rdf:resource="http://dati.beniculturali.it/cis/Site"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/location/SiteType"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un contenitore fisico al tipo di contenitore fisico, per permetterne la corretta e precisa individuazione.</rdfs:comment>
        <rdfs:label xml:lang="it">ha tipo di contenitore fisico</rdfs:label>
        <rdfs:label xml:lang="en">has site type</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>LC/LDC/LDCT; LC/LDC/LDCQ</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasTimeIndexedQualifiedLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasTimeIndexedQualifiedLocation">
        <rdfs:range rdf:resource="https://w3id.org/arco/location/TimeIndexedQualifiedLocation"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità a una sua localizzazione qualificata nel tempo.</rdfs:comment>
        <rdfs:label xml:lang="it">ha localizzazione qualificata nel tempo</rdfs:label>
        <rdfs:label xml:lang="en">has time indexed qualified location</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">LC; LA; per normativa F: anche LR</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasToponymInTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasToponymInTime">
        <rdfs:range rdf:resource="https://w3id.org/arco/location/ToponymInTime"/>
        <rdfs:comment xml:lang="it">Questa proprietà permette di collegare un luogo a una denominazione che cambia nel tempo.</rdfs:comment>
        <rdfs:label xml:lang="it">ha toponimo nel tempo</rdfs:label>
        <rdfs:label xml:lang="en">has toponym in time</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag>LC/PVL; LA/PRL</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasTypeOfContext -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/location/hasTypeOfContext">
        <rdfs:range rdf:resource="https://w3id.org/arco/location/TypeOfContext"/>
        <rdfs:comment xml:lang="it">Questa proprietà permette di collegare un bene culturale al tipo di contesto (urbano, suburbano, territoriale, subacqueo) in cui si trova.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a cultural property to the type of context in which the cultural property is located.</rdfs:comment>
        <rdfs:label xml:lang="it">ha tipo di contesto geografico</rdfs:label>
        <rdfs:label xml:lang="en">has type of geographical context</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>LC/PVZ; LA/PRZ</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/italia/onto/TI/atTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/TI/atTime"/>
    


    <!-- ttps://w3id.org/italia/onto/TI/atTime -->

    <owl:ObjectProperty rdf:about="ttps://w3id.org/italia/onto/TI/atTime"/>
    


