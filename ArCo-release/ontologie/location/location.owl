    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dati.gov.it/onto/l0/name -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/l0/name"/>
    


    <!-- https://w3id.org/arco/location/alt -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/arco/location/alt">
        <rdfs:domain rdf:resource="https://w3id.org/arco/location/Altitude"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la coordinata z di un oggetto, che ne definisce l&apos;altitudine rispetto al livello del mare.</rdfs:comment>
        <rdfs:label xml:lang="en">altitude</rdfs:label>
        <rdfs:label xml:lang="it">altitudine</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: GE/GEC/GECZ; normative precedenti: GP/GPC/GPCL</arco-core:iccdNormTag>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/arco/location/lat -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/arco/location/lat">
        <rdfs:domain rdf:resource="https://w3id.org/arco/location/Coordinates"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la coordinata y, ovvero la latitudine.</rdfs:comment>
        <rdfs:label xml:lang="en">latitude</rdfs:label>
        <rdfs:label xml:lang="it">latitudine</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag>GP/GPD/GPDP/GPDPY; GA/GAD/GADP/GADPY; GL/GLD/GLDP/GLDPY; ICCD Normativa Trasversale 4.00: GE/GEC/GECY</arco-core:iccdNormTag>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/arco/location/locationDetails -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/arco/location/locationDetails">
        <rdfs:domain rdf:resource="https://w3id.org/arco/location/TimeIndexedQualifiedLocation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Questa proprietà è utilizzata per rappresentare informazioni sui percorsi che permettono l’accesso al bene o che lo delimitano, o ulteriori specifiche per la localizzazione del bene.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is used to represent other paths and details about the cultural property location.</rdfs:comment>
        <rdfs:label xml:lang="it">dettagli della localizzazione</rdfs:label>
        <rdfs:label xml:lang="en">location details</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag>LC/PVC/PVCV</arco-core:iccdNormTag>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/arco/location/long -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/arco/location/long">
        <rdfs:domain rdf:resource="https://w3id.org/arco/location/Coordinates"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la coordinata x, ovvero la longitudine.</rdfs:comment>
        <rdfs:label xml:lang="en">longitude</rdfs:label>
        <rdfs:label xml:lang="it">longitudine</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag>GP/GPD/GPDP/GPDPX; GA/GAD/GADP/GADPX; GL/GLD/GLDP/GLDPX; ICCD Normativa Trasversale 4.00: GE/GEC/GECX</arco-core:iccdNormTag>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/arco/location/maxAlt -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/arco/location/maxAlt">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/location/alt"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/location/Altitude"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
