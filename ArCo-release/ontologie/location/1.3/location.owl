        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/TimeIndexedTypedLocation"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/Site"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una localizzazione tipizzata nel tempo al contenitore fisico cui si riferisce.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links a typified location over time to its physical container (Site).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
        <rdfs:label xml:lang="en">at site</rdfs:label>
        <rdfs:label xml:lang="it">presso contenitore fisico</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <a-cat:iccdNormTag xml:lang="it">ICCD Normativa Trasversale: LC/LDC; LA/PRC</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/containsCadastralIdentity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/containsCadastralIdentity">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/location/isContainedInCadastre"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/Cadastre"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/CadastralIdentity"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega il catasto all identità catastale contenuta in esso.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links a cadastre to the cadastral identity containded in it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
        <rdfs:label xml:lang="en">contains cadastral identity</rdfs:label>
        <rdfs:label xml:lang="it">contiene identità catastale</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasAltitude -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasAltitude">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/location/isAltitudeOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/Coordinates"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/Altitude"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega le coordinate relative alla georeferenziazione di un oggetto (Geometry) alla sua altitudine.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links the georeferencing coordinates of an object (Geometry) to its altitude.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
        <rdfs:label xml:lang="it">ha altitudine</rdfs:label>
        <rdfs:label xml:lang="en">has altitude</rdfs:label>
        <owl:deprecated rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:deprecated>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <a-cat:iccdNormTag xml:lang="it">quota minima: GP/GPC/GPCI - GA/GAQ/GAQI - GL/GLQ/GLQI - MT/MTA/MTAM; quota massima: GP/GPC/GPCS - GA/GAQS - GL/GLQ/GLQS - MT/MTA/MTAX; quota relativa: MT/MTA/MTAR; coordinata z: 4.00 GE/GEC/GECZ - 3.00GP/GPC/GPCL</a-cat:iccdNormTag>
        <core:constructQuery>prefix clv: &lt;https://w3id.org/italia/onto/CLV/&gt; 

CONSTRUCT {
?geo clv:alt ?alt .
}

WHERE {
?geo a-loc:hasCoordinates ?coor .
?coor a-loc:hasAltitude ?altclass .
?altclass a-loc:alt ?alt .
}</core:constructQuery>
        <core:replacedBy rdf:resource="https://w3id.org/italia/onto/CLV/alt"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasBaseMap -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasBaseMap">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/location/isBaseMapOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Geometry"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/BaseMap"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una georeferenziazione di un oggetto alla base cartografica eventualmente utilizzata.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links an object georeferencing to the base map possibly used.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
        <rdfs:label xml:lang="it">ha base cartografica</rdfs:label>
        <rdfs:label xml:lang="en">has base map</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <a-cat:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: GE/GPB; versioni precedenti: GP/GPB - GA/GAB - GL/GLB</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasCadastralCity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasCadastralCity">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/location/hasCadastralEntity"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/location/isCadastralCityOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/CadastralIdentity"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/City"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;identità catastale di un&apos;entità al comune a cui è intestato il foglio di mappa catastale, riportato nella forma attestata nel catasto.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links a cadastral identity of an entity to the municipality to which the cadastral map is registered, as attested by the cadastre.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
        <rdfs:label xml:lang="it">ha città catastale</rdfs:label>
        <rdfs:label xml:lang="en">has cadastral city</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <a-cat:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: CS/CTS/CTSC. - Normativa RA v.2: RP/LGC/LGCC</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasCadastralEntity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasCadastralEntity">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/location/isCadastralEntityOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/CadastralIdentity"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/CadastralEntity"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;identità catastale di un&apos;entità a una delle entità catastali, intese come oggetti spaziali, che la definiscono.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links a cadastral identity of an entity to one of the cadastral entities, understood as spatial objects, that define it.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
        <rdfs:label xml:lang="it">ha entità catastale</rdfs:label>
        <rdfs:label xml:lang="en">has cadastral entity</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa Traversale 4.00: CS/CTS, CS/CTS/CTSF, CTSN; LS/CSS. LG/LGC</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasCadastralIdentity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasCadastralIdentity">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/location/isCadastralIdentityOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/CadastralIdentity"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale all&apos;insieme delle informazioni catastali che lo riguardano, relativamente a una delle sue localizzazioni (storica, attuale, di rinvenimento, ecc.)</rdfs:comment>
        <rdfs:comment xml:lang="en">This property links a cultural property to all its cadastral information related to one of its locations: i.e. historical location, current location, finding location, etc.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location"/>
        <rdfs:label xml:lang="it">ha identità catastale</rdfs:label>
        <rdfs:label xml:lang="en">has cadastral identity</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa Traversale 4.00: CS; LS.
Normativa A e PG v.2.: UB
Normativa RA v.2: RP/LGC</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasCadastreType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasCadastreType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/core/hasType"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/location/isCadastreTypeOf"/>
