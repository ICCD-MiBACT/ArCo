        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/arco/ontology/location/hasTimeIndexedTypedLocation"/>
            <rdf:Description rdf:about="https://w3id.org/arco/ontology/location/atLocation"/>
            <rdf:Description rdf:about="https://w3id.org/italia/onto/CLV/hasAddress"/>
        </owl:propertyChainAxiom>
