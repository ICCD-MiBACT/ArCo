    <!-- https://w3id.org/arco/ontology/location/areCoordinatesOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/areCoordinatesOf">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/location/hasCoordinates"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/Coordinates"/>
