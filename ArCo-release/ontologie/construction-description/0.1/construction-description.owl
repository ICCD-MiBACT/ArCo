        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
        <rdfs:label xml:lang="it">ha struttura del tetto</rdfs:label>
        <rdfs:label xml:lang="en">has roof structure</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>Normativa A: CP</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasShadingDevice -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasShadingDevice">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isShadingDeviceOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/construction-description/Window"/>
