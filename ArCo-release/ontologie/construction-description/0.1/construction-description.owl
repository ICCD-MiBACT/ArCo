    <!-- https://w3id.org/arco/ontology/construction-description/hasArchitecturalOrder -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasArchitecturalOrder">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/denotative-description/hasTechnicalCharacteristic"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isArchitecturalOrderOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/construction-description/VerticalElement"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction-description/ArchitecturalOrder"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità all&apos;ordine architettonico.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects an entity to the architectural order.</rdfs:comment>
        <rdfs:label xml:lang="it">ha ordine architettonico</rdfs:label>
        <rdfs:label xml:lang="en">has architectural order</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasConstructionElement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasConstructionElement">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/core/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isConstructionElementOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/construction-description/Construction"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction-description/ConstructionElement"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una costruzione all&apos;elemento della costruzione.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects a construction to the construction element.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
        <rdfs:label xml:lang="it">ha elemento della costruzione</rdfs:label>
        <rdfs:label xml:lang="en">has construction element</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasConstructionSpace -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasConstructionSpace">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/core/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isConstructionSpaceOf"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una costruzione ai suoi spazi</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a construction to its construction space</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
        <rdfs:label xml:lang="it">ha spazio della costruzione</rdfs:label>
        <rdfs:label xml:lang="en">has construction space</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa MA, CA, SI, SAS: DA/SPA</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasCovering -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasCovering">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isCoveringOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/arco/ontology/construction-description/Construction"/>
                    <rdf:Description rdf:about="https://w3id.org/arco/ontology/construction-description/ConstructionPart"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction-description/Covering"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità al rivestimento  .</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects an entity to a covering .</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
        <rdfs:label xml:lang="it">ha rivestimento</rdfs:label>
        <rdfs:label xml:lang="en">has covering</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa: PV e CP</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasDesign -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasDesign">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/l0/hasDescription"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isDesignOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/arco/ontology/construction-description/Construction"/>
                    <rdf:Description rdf:about="https://w3id.org/arco/ontology/construction-description/ConstructionSpace"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction-description/ConstructionDesign"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità al design.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects an entity to a design .</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
        <rdfs:label xml:lang="it">ha progetto</rdfs:label>
        <rdfs:label xml:lang="en">has design</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasFlooringLayout -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasFlooringLayout">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/l0/hasDescription"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isFlooringLayoutOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/construction-description/Flooring"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction-description/FlooringLayout"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la pavimentazione allo schema di posa.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects the flooring to the flooring layout.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
        <rdfs:label xml:lang="it">ha schema di posa</rdfs:label>
        <rdfs:label xml:lang="en">has flooring layout</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>Normativa A: PV/PVM/PVMS</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasInteriorSubdivision -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasInteriorSubdivision">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isInteriorSubdivisionOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/arco/ontology/construction-description/Construction"/>
                    <rdf:Description rdf:about="https://w3id.org/arco/ontology/construction-description/ConstructionSpace"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction-description/InteriorSubdivision"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una costruzione alla suddivisione interna .</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects a construction to the interior subdivision.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
        <rdfs:label xml:lang="it">ha suddivisione interna</rdfs:label>
        <rdfs:label xml:lang="en">has interior subdivision</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>ICCD Normativa A: SI/SII</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasLayout -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasLayout">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isLayoutOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/construction-description/Construction"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction-description/Layout"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega il design allo schema iconografico.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects the design to the layout.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
        <rdfs:label xml:lang="en">has layout</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>Normativa A: PN/PNT/PNTS e PN/PNT/PNTF</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasModeOfIrrigation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasModeOfIrrigation">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isModeOfIrrigationOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/construction-description/WaterSystem"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction-description/ModeOfIrrigation"/>
        <rdfs:comment xml:lang="en">This property connects a water system to the mode of irrigation.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
        <rdfs:label xml:lang="it">ha modalità di irrigazione</rdfs:label>
        <rdfs:label xml:lang="en">has mode of irrigation</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag>Normativa PG: CA/CAI</a-cat:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasNaturalElement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasNaturalElement">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/core/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isNaturalElementOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/construction-description/Construction"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction-description/NaturalElement"/>
        <rdfs:comment xml:lang="en">Questa proprietà collega una costruzione a un elemento naturale</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a construction with a natural element</rdfs:comment>
        <rdfs:label xml:lang="it">ha elemento naturale</rdfs:label>
        <rdfs:label xml:lang="en">has natural element</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <a-cat:iccdNormTag rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasOpeningSystem -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasOpeningSystem">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/construction-description/isOpeningSystemOf"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction-description/OpeningSystem"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una porta o una finestra al sistema di apertura.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property connects a door or a window to the opening system.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
        <rdfs:label xml:lang="it">ha sistema di apertura</rdfs:label>
        <rdfs:label xml:lang="en">has opening system</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasPreviousConstructionOrElement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasPreviousConstructionOrElement">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/core/hasPart"/>
