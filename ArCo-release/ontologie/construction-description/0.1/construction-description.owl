        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/construction-description/Construction"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/construction-description/Construction"/>
        <rdfs:comment xml:lang="en">This property relates a construction with its previous construction or element</rdfs:comment>
        <rdfs:comment xml:lang="it">questa proprietà collega una costruzione a un suo precedente elemento o costruzione</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontologyconstruction-description"/>
        <rdfs:label xml:lang="it">ha elemento o costruzione precedente</rdfs:label>
        <rdfs:label xml:lang="en">has previous construction or element</rdfs:label>
        <owl:versionInfo xml:lang="it">instabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/construction-description/hasRoofStructure -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/construction-description/hasRoofStructure">
