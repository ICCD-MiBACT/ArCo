        <testannotationschema:hasSPARQLQueryUnitTest>PREFIX mp: &lt;https://w3id.org/arco/ontology/movable-property/&gt;
PREFIX arco: &lt;https://w3id.org/arco/ontology/arco/&gt;
PREFIX core: &lt;https://w3id.org/arco/ontology/core/&gt;
SELECT ?specimen ?taxon
WHERE{
?specimen core:hasClassificationInTime ?classification .
?classification  mp:hasTaxon ?taxon .
?taxon a mp:Cultivar.
}
limit 100
