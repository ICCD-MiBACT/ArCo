        <testannotationschema:hasSPARQLQueryUnitTest xml:lang="en">PREFIX arco-cd: &lt;https://w3id.org/arco/ontology/context-description/&gt;

SELECT *
WHERE{
  ?cultprop arco-cd:hasSexEstimate ?SexEst .
  ?SexEst arco-cd:estimatedSex ?sex. 
  ?cultprop arco-cd:hasAgeOfDeathEstimate ?DeathAge . 
  ?DeathAge arco-cd:estimatedAgeOfDeath ?age.
  ?cultprop arco-cd:hasFindingContext ?Context .
  ?Context arco-cd:hasFindingContextType ?ContextType . 
  ?cultprop arco-cd:hasBiologicalTaxonomy ?BioTax .
  ?BioTax arco-cd:hasSpecies ?Species .    
  
  FILTER regex(str(?sex), &quot;F&quot;, &quot;i&quot;)
  FILTER regex(str(?age), &quot;21&quot;) 
}
LIMIT 100</testannotationschema:hasSPARQLQueryUnitTest>
