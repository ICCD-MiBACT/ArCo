@prefix rs:   <http://www.w3.org/2001/sw/DataAccess/tests/result-set#> .
@prefix ex: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Identifier/CQ/CQ2/dataset_cq2.ttl/> .
@prefix test: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Identifier/CQ/CQ2/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .

test:testcase_cq2.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "Which organization has issued the cp X identifier? " ;
	owlunit:hasSPARQLUnitTest "prefix ex: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Identifier/CQ/CQ2/dataset_cq2.ttl/> prefix a-dd: <https://w3id.org/arco/ontology/denotative-description/> prefix arco: <https://w3id.org/arco/ontology/arco/>  prefix core: <https://w3id.org/arco/ontology/core/>  SELECT DISTINCT select ?org { ?x core:hasIdentifier ?id . ?id core:issuedBy ?org . }" ;
	owlunit:hasInputData <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Identifier/CQ/CQ2/dataset_cq2.ttl> ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
	owlunit:hasExpectedResult [ a                  rs:ResultSet ;
					  rs:resultVariable  "org" ;
					  rs:size            "1"^^xsd:int ;
					  rs:solution        [ rs:binding  [ rs:value     ex:Organization1 ;
									     rs:variable  "org"
									   ]
                                 ]
					] ;
	owlunit:testsOntology <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Identifier/Identifier.ttl> .
