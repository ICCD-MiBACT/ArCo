@prefix rs:   <http://www.w3.org/2001/sw/DataAccess/tests/result-set#> .
@prefix ex: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Subject/CQ/CQ1/dataset_cq1.ttl/> .
@prefix test: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Subject/CQ/CQ1/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .

test:testcase_cq1.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "What are the subjects represented in cultural property x?" ;
	owlunit:hasSPARQLUnitTest "prefix ex: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Subject/CQ/CQ1/dataset_cq1.ttl/> prefix a-cd: <https://w3id.org/arco/ontology/context-description/> SELECT DISTINCT ?concept WHERE{ ?x a-cd:hasSubject ?subject . ?subject core:isClassifiedBy ?concept . }" ;
	owlunit:hasInputData <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Subject/CQ/CQ1/dataset_cq1.ttl> ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
	owlunit:hasExpectedResult [ a                  rs:ResultSet ;
					  rs:resultVariable  "concept" ;
					  rs:size            "1"^^xsd:int ;
					  rs:solution        [ rs:binding  [ rs:value     ex:concept1 ;
									     rs:variable  "concept"
									   ]
                                 ]
					] ;
	owlunit:testsOntology <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Subject/Subject.ttl> .
