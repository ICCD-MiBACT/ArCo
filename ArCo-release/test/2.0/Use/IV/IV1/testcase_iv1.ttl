@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .
@prefix ex: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Use/IV/IV1/dataset_iv1.ttl/> .
@prefix test: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Use/IV/IV1/> .
@prefix onto: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Use/Use.ttl> .
@prefix core: <https://w3id.org/arco/ontology/core/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

test:testcase_iv1.ttl a owlunit:InferenceVerification ;
	owlunit:hasInputData test:dataset_iv1.ttl;
	owlunit:hasSPARQLUnitTest "PREFIX core: <https://w3id.org/arco/ontology/core/> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX  ex: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Use/IV/IV1/dataset_iv1.ttl/> ASK { ex:Use rdf:type core:EventOrSituationInTime }" ;
	owlunit:hasExpectedResult true ;
	owlunit:hasReasoner owlunit:HermiT ;
 	owlunit:testsOntology onto: .
