 	owlunit:hasCompetencyQuestion "What are accessories associated to the cultural property cp1?" ;
 	owlunit:hasSPARQLUnitTest "prefix a-dd: <https://w3id.org/arco/ontology/denotative-description/> prefix ex: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Accessory/CQ/CQ1/dataset_cq1.ttl> prefix onto: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Accessory/Accessory.ttl> SELECT DISTINCT ?accessory WHERE{ ex:cp1 a-dd:hasAccessory ?accessory. } limit 100" ;
	owlunit:hasInputData ex: ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
	owlunit:hasExpectedResult [ a                  rs:ResultSet ;
					  rs:resultVariable  "accessory" ;
					  rs:size            "1"^^xsd:int ;
					  rs:solution        [ rs:binding  [ rs:value     ex:Accessory1 ;
									     rs:variable  "accessory"
									   ]
                                 ]
					] ;
