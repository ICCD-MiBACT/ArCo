	owlunit:hasInputData test:dataset_iv1.ttl;
	owlunit:hasSPARQLUnitTest "PREFIX core: <https://w3id.org/arco/ontology/core/> PREFIX  ex: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Accessory/IV/IV1/dataset_iv1.ttl/> ASK { ex:accessory1 core:hassType ex:accessoryType1 }" ;
	owlunit:hasExpectedResult true ;
	owlunit:hasReasoner owlunit:HermiT ;
 	owlunit:testsOntology core: .
