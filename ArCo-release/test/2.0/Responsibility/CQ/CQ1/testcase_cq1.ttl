@prefix rs:   <http://www.w3.org/2001/sw/DataAccess/tests/result-set#> .
@prefix ex: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Responsibility/CQ/CQ1/dataset_cq1.ttl/> .
@prefix test: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Responsibility/CQ/CQ1/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix owlunit: <https://w3id.org/OWLunit/ontology/> .

test:testcase_cq1.ttl a owlunit:CompetencyQuestionVerification ;
 	owlunit:hasCompetencyQuestion "Who is the author of cultural property x?" ;
	owlunit:hasSPARQLUnitTest "prefix ex: <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Responsibility/CQ/CQ1/dataset_cq1.ttl/> prefix a-cd: <https://w3id.org/arco/ontology/context-description/> select distinct ?author  where { ?cp a-cd:hasResponsibility ?resp. ?resp a-cd:hasInterventionRole ?role ; a-cd:hasAgentWithResponsibility ?author . FILTER regex(?role, "autore", "i") }" ;
	owlunit:hasInputData <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Responsibility/CQ/CQ1/dataset_cq1.ttl> ;
	owlunit:hasInputTestDataCategory owlunit:ToyDataset ;
	owlunit:hasExpectedResult [ a                  rs:ResultSet ;
					  rs:resultVariable  "author" ;
					  rs:size            "1"^^xsd:int ;
					  rs:solution        [ rs:binding  [ rs:value     ex:artista1 ;
									     rs:variable  "author"
									   ]
                                 ]
					] ;
	owlunit:testsOntology <https://raw.githubusercontent.com/ICCD-MiBACT/ArCo/DEV-1.3.0/ArCo-release/test/2.0/Responsibility/Responsibility.ttl> .
